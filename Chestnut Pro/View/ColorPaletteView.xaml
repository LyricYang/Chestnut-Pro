                        <Button Style="{StaticResource MaterialDesignRaisedButton}"
                                Content="Convert"
                                Click="RGBConvert"
                                Height="50"
                                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                                Margin="285,540,125,10" />
                    </Grid>
                </Grid>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
